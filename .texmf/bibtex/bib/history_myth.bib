@article{Denevan:1992kl,
	Author = {Denevan, William M.},
	Cr = {Copyright \copyright 1992 Association of American Geographers},
	Date-Added = {2010-08-30 02:22:41 -0400},
	Date-Modified = {2010-08-30 02:22:41 -0400},
	Isbn = {00045608},
	Journal = {Annals of the Association of American Geographers},
	M1 = {ArticleType: research-article / Issue Title: The Americas before and after 1492: Current Geographical Research / Full publication date: Sep., 1992 / Copyright {\copyright}1992 Association of American Geographers},
	Number = {3},
	Pages = {369--385},
	Publisher = {Taylor \& Francis, Ltd. on behalf of the Association of American Geographers},
	Title = {The Pristine Myth: The Landscape of the Americas in 1492},
	Ty = {JOUR},
	Url = {http://www.jstor.org/stable/2563351},
	Volume = {82},
	Year = {Sep., 1992},
	Annote = {The myth persists that in 1492 the Americas were a sparsely populated wilderness, "a world of barely perceptible human disturbance." There is substantial evidence, however, that the Native American landscape of the early sixteenth century was a humanized landscape almost everywhere. Populations were large. Forest composition had been modified, grasslands had been created, wild-life disrupted, and erosion was severe in places. Earthworks, roads, fields, and settlements were ubiquitous. With Indian depopulation in the wake of Old World disease, the environment recovered in many areas. A good argument can be made that the human presence was less visible in 1750 than it was in 1492.},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.jstor.org/stable/2563351}}

@article{Gosden:1998ai,
	Author = {Gosden, Chris and Lock, Gary},
	Cr = {Copyright \copyright 1998 Taylor \& Francis, Ltd.},
	Date-Added = {2010-08-30 02:22:41 -0400},
	Date-Modified = {2010-08-30 02:22:41 -0400},
	Isbn = {00438243},
	Journal = {World Archaeology},
	M1 = {ArticleType: research-article / Issue Title: The Past in the Past: The Reuse of Ancient Monuments / Full publication date: Jun., 1998 / Copyright {\copyright}1998 Taylor \& Francis, Ltd.},
	Number = {1},
	Pages = {2--12},
	Publisher = {Taylor \& Francis, Ltd.},
	Title = {Prehistoric Histories},
	Ty = {JOUR},
	Url = {http://www.jstor.org/stable/125005},
	Volume = {30},
	Year = {Jun., 1998},
	Annote = {This paper discusses prehistoric forms of history. We propose that all prehistoric societies oriented their actions in the present with the past in mind. The task for archaeologists is to understand the sorts of histories at work in different periods. We propose a distinction between genealogical history, where the past is created through links to known ancestors, and mythical history, where a less well-known past is evoked. These two forms of history coexist in all societies with implications for the manner in which continuities and change are handled. We illustrate the insights that these ideas may provide through a case study from the late Bronze Age to Romano-British periods on the Ridgeway in south Oxfordshire, southern England.},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.jstor.org/stable/125005}}

@article{Spiro:1979tg,
	Author = {Spiro, Melford E.},
	Cr = {Copyright \copyright 1979 American Anthropological Association},
