@mastersthesis{Angulo:2009sf,
	Abstract = {Abstract Early Restoration Literature, into the Age of Enlightenment, was the abode of elite men. Women, when they were allowed to write had to keep to the very tight moral leeshes set down in the moral handbooks of the day. However, the moral code that was meant to protect women was also used to defile a woman's character. For this reason, female writers in the pre- and post-Restoration eras, to show their disapproval of the morality of the age --- that of sensibility --- where at risk of losing their standing in society. For this purpose the novel became a battlefield for satirical writers. The greatest of these battles pitted the male father, against the artificiality of the female daughter.},
	Author = {Luis Enrique Mendez Angulo},
	Date-Added = {2010-02-06 00:06:11 -0500},
	Date-Modified = {2010-02-06 00:06:11 -0500},
	Month = {March},
