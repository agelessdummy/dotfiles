	Jstor_Formatteddate = {May, 1968},
	Number = {2},
	Pages = {284--297},
	Publisher = {Modern Language Association},
	Title = {Murder as a Fine Art: Basic Connections between Poe's Aesthetics, Psychology, and Moral Vision},
	Url = {http://www.jstor.org/stable/1261183},
